{
  "expo": {
    "name": "Dana",
    "slug": "Dana",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/brand/app/icon.png",

 
    "userInterfaceStyle": "light",

    "newArchEnabled": true,

   
    "splash": {
      "image": "./assets/images/brand/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true,
 
      "userInterfaceStyle": "light"
    },

    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/brand/app/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
   
      "userInterfaceStyle": "light"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/brand/app/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
         
          "backgroundColor": "#ffffff",
          "image": "./assets/images/brand/app/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    }
  }
}
